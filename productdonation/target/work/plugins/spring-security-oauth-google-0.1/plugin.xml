<plugin name='spring-security-oauth-google' version='0.1' grailsVersion='1.2.2 &gt; *'>
  <author>Mihai Cazacu, Enrico Comiti, Alexey Zhokhov</author>
  <authorEmail>donbeave@gmail.com</authorEmail>
  <title>Google for Spring Security OAuth plugin</title>
  <description>Integrate [Google|http://www.google.com] to [Spring Security OAuth plugin|http://grails.org/plugin/spring-security-oauth].
</description>
  <documentation>https://github.com/donbeave/grails-spring-security-oauth-google</documentation>
  <type>SpringSecurityOauthGoogleGrailsPlugin</type>
  <resources>
    <resource>grails.plugin.springsecurity.oauth.GoogleSpringSecurityOAuthService</resource>
  </resources>
</plugin>